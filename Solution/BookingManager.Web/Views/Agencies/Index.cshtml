@using BookingManager.Web.Models
@model AgenciesViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-10">
        <h2>Agencias</h2>
        <ol class="breadcrumb">
            <li>
                <a>Tablero</a>
            </li>
            <li class="active">
                <strong>Agencias asociadas</strong>
            </li>
        </ol>
    </div>
    <div class="col-sm-2">

    </div>
</div>

<br />
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5>Listado de agencias asociadas  <small>Administre sus agencias asociadas.</small></h5>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
        <div class="row">
            <div class="col-sm-12">
                <div id="divAgencies">
                    @Html.Partial("_Agencies", Model)
                </div>
            </div>
        </div>
    </div>
</div>

@Scripts.Render("~/plugins/toastr")
@Scripts.Render("~/app/linkedagencies");

